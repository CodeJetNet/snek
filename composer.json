{
  "name": "jhoughtelin/battlesnek",
  "license": "MIT",
  "type": "project",
  "autoload": {
    "psr-4": {
      "PhpTek\\BattleSnek\\": "./src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "PhpTek\\Tests\\": "./tests"
    }
  },
  "require": {
    "php": "^8.2",
    "ext-json": "*",
    "slim/slim": "^4.12",
    "slim/http": "^1.2",
    "http-interop/http-factory-guzzle": "^1.2",
    "slim/psr7": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "squizlabs/php_codesniffer": "^3.6",
    "phpstan/phpstan": "^1.10"
  },
  "scripts": {
    "test": "phpunit",
    "check-style": "phpcs -p --standard=phpcs.xml src",
    "fix-style": "phpcbf -p --standard=phpcs.xml src"
  }
}
